set terminal pngcairo dashed size 1000,800
set output 'scl64size_thesis1.png'
set title "WER x SNR (SCL 64 size for AWGN channel)"
set key left bottom
set key samplen 12 spacing 2 font "30,15"
set xlabel "Signal-to-noise ratio [dB]"
set ylabel "Word error rate"
set logscale y 10
set yrange [1e-6:1]
set xrange [1.00:5.00]
set xtics 0.5
set format y "10^{%L}"
set grid mytics lt 1 lw 1 layerdefault back
set grid xtics lt 0 lw 1 layerdefault back
plot \
'graph64thesis1.txt' u 1:2 dt (10,10) lt 4 lw 3 pt 6 ps 1 with linespoints title 'Polar Subcode (64,32,12), L=32',\
'graph64thesis1.txt' u 1:3 dt 1 lt 1 lw 3 pt 5 ps 1 with linespoints title 'Polar Subcode (64,32,12) optimized weight spectrum, L=32'
pause -1 "Hit any key to continue"
